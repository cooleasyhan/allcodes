SELECT * FROM FND_CONC_REQ_SUMMARY_V V WHERE V.REQUESTED_START_DATE > TO_DATE('2015/10/12 14:53:02','YYYY/MM/DD HH24:MI:SS')
ORDER BY V.REQUESTED_START_DATE␀.Get_Ddl(Object_Type => t.Object_Type,
                             NAME        => t.Object_Name,
                             SCHEMA      => t.Owner) Ddl
  FROM Dba_Objects t
 WHERE t.Object_Name LIKE 'CUX%'
   AND t.Object_Type IN ('TABLE', 'VIEW', 'PACKAGE', 'TRIGGER', 'SEQUENCE')
   AND t.Object_Name in ( 'CUXADMINR_IMP_PKG','CUXADMINR_PKG')
--ORDER BY t.Object_Type␀637')␀
                  WHEN v.Party_Name LIKE '%²ÊÆ±' THEN
                   '²ÊÆ±'
                  ELSE
                   'others'
                END pname,
                CASE
                  WHEN Ou.Name LIKE '%ÊÖÓÎ%' THEN
                   'ÊÖÓÎ'
                  WHEN Ou.Name LIKE '%·Ç¶¨·Ñ%' THEN
                   '·Ç¶¨·Ñ'
                  WHEN Ou.Name LIKE '%Ö§¸¶ÖÐÐÄ%' THEN
                   'Ö§¸¶ÖÐÐÄ'
                  WHEN Ou.Name LIKE '%Ò³ÓÎ%' THEN
                   'Ò³ÓÎ'
                  WHEN Ou.Name LIKE '%²ÊÆ±%' THEN
                   '²ÊÆ±'
                  ELSE
                   'others'
                END ouname
  FROM Cux_Cust_Acct_Sites    s,
       Cux_Customer_b_v       v,
       Hz_Cust_Acct_Sites_All Sa,
       Hr_Operating_Units     Ou
 WHERE s.Cust_Account_Id = v.Cust_Account_Id
   AND s.Cust_Acct_Site_Id = Sa.Cust_Acct_Site_Id
   AND s.Result_Status = 'S'
   AND Ou.Organization_Id = Sa.Org_Id
   AND v.Acc_Attr1 IS NOT NULL )rst
   where rst.pname <> rst.ouname

-- AND v.Party_Name LIKE '%\_%' ESCAPE '\'␀,
       Cr.Org_Id,
       Cr.Unique_Reference,
       Cr.Remittance_Bank_Account_Id,
       Cr.Remit_Bank_Acct_Use_Id,

       Cr.Legal_Entity_Id
  FROM Ar_Cash_Receipts_All        Cr,
       Ar_Cash_Receipt_History_All Ach,
       Hz_Cust_Accounts            Cust,
       Hz_Parties                  Party,
       Hz_Cust_Site_Uses_All       Site_Uses,
       Ce_Bank_Accounts            Cba,
       Ce_Bank_Acct_Uses_All       Remit_Bank,
       Ce_Bank_Branches_v          Bb,
       Ar_Receipt_Methods          Rec_Method,
       Ar_Receipt_Classes          Rc

 WHERE Cr.Cash_Receipt_Id = Ach.Cash_Receipt_Id(+)
   AND Cr.Org_Id = Ach.Org_Id(+)
   AND Ach.First_Posted_Record_Flag(+) = 'Y'
   AND Cr.Pay_From_Customer = Cust.Cust_Account_Id(+)
   AND Cust.Party_Id = Party.Party_Id(+)
   AND Cr.Customer_Site_Use_Id = Site_Uses.Site_Use_Id(+)
   AND Cr.Org_Id = Site_Uses.Org_Id(+)
   AND Remit_Bank.Bank_Acct_Use_Id(+) = Cr.Remit_Bank_Acct_Use_Id
   AND Remit_Bank.Org_Id(+) = Cr.Org_Id
   AND Remit_Bank.Bank_Account_Id = Cba.Bank_Account_Id(+)
   AND Bb.Branch_Party_Id(+) = Cba.Bank_Branch_Id
   AND Cr.Receipt_Method_Id = Rec_Method.Receipt_Method_Id
   AND Rec_Method.Receipt_Class_Id = Rc.Receipt_Class_Id␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
