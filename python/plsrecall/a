alter trigger ra_cust_trx_line_gl_dist_bri disable␀d     NUMBER,
  interface_run_id  NUMBER,
  iface_id          NUMBER,
  interface_key     VARCHAR2(50),
  status            VARCHAR2(50),
  error_message     VARCHAR2(4000),
  customer_name     VARCHAR2(360),
  customer_known_as VARCHAR2(240),
  account_number    VARCHAR2(30),
  ou_name           VARCHAR2(240),
  is_bill_to        VARCHAR2(1),
  is_ship_to        VARCHAR2(1),
  last_update_date  DATE,
  last_updated_by   NUMBER(15),
  creation_date     DATE,
  created_by        NUMBER(15),
  last_update_login NUMBER(15)
)␀Org_Id)␀d,
               App.Trx_Number,
               To_Char(App.Gl_Date, 'MM-YYYY') Gl_Period,
               App.Code_Combination_Id,
               SUM(App.Amount_Applied) Amount_Applied
          FROM Cux_Ar_Cash_App_b_v App
         WHERE App.Trx_Number IS NOT NULL
         GROUP BY App.Cash_Receipt_Id,
                  App.Trx_Number,
                  To_Char(App.Gl_Date, 'MM-YYYY'),
                  App.Code_Combination_Id
        HAVING SUM(App.Amount_Applied) <> 0) App,
       Cux_Ar_Cash_Receipts_b_v Ar
 WHERE App.Cash_Receipt_Id = Ar.Cash_Receipt_Id␀ = 'Y'
   AND Application_Id = 222
   AND EXISTS
 (SELECT Gas.Ledger_Id
          FROM Gl_Access_Set_Ledgers Gas
         WHERE Gas.Access_Set_Id = Fnd_Profile.Value('GL_ACCESS_SET_ID')
           AND Gas.Ledger_Id = Cgsp.Ledger_Id)
--AND ledger_id = fnd_profile.value('GL_SET_OF_BKS_ID')
 ORDER BY Period_Year DESC, Period_Name DESC␀ll Aid
               WHERE Ach.Status_Code NOT IN ('5', '8')
                 AND Ach.Customer_Name = Aps.Vendor_Name(+)
                 AND Api.Invoice_Id = Aid.Invoice_Id(+)
                 AND Aps.Vendor_Id = Api.Vendor_Id(+)) rst␀Attribute2, --attribute2
       Jel.Attribute3   Attribute3, --attribute3
       Jel.Attribute4   Attribute4, --attribute4
       Jel.Attribute5   Attribute5, --attribute5
       Jel.Attribute6   Attribute6, --attribute6
       Jel.Attribute7   Attribute7, --attribute7
       Jel.Attribute8   Attribute8, --attribute8
       Jel.Attribute9   Attribute9, --attribute9
       Jel.Attribute10  Attribute10, --attribute10
       Jel.Attribute11  Attribute11,
       Jel.Attribute12  Attribute12,
       Jel.Attribute13  Attribute13,
       Jel.Attribute14  Attribute14,
       Jel.Attribute15  Attribute15,
       Jel.Attribute16  Attribute16,
       Jel.Attribute17  Attribute17,
       Jel.Attribute18  Attribute18,
       Jel.Attribute19  Attribute19,
       Jel.Attribute20  Attribute20
        FROM Gl_Je_Lines          Jel,
             Gl_Je_Headers        Jeh,
             Gl_Code_Combinations Cc,
             Gl_Ledgers           Gl
       WHERE 1 = 1
         AND Gl.Ledger_Id = Jeh.Ledger_Id
         AND Jel.Je_Header_Id = Jeh.Je_Header_Id
         AND Cc.Code_Combination_Id = Jel.Code_Combination_Id
            --  AND jel.je_header_id = in_header_id
            --AND cc.segment1 = p_com_segment
        -- AND Jeh.Je_Source = 'Global Intercompany'
         AND GL.NAME LIKE '%QY%'␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
