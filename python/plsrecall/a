  FROM V$lock          l,
       V$lock_Type     Lt,
       V$locked_Object Lo,
       All_Objects     Obj,
       V$session       s
 WHERE 1 = 1
   AND l.Type = Lt.Type
   AND Lo.Session_Id = s.Sid
