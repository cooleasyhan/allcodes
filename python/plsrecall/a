SELECT *
  FROM (SELECT Gl.Name ·ÖÀàÕÊ,
               Gjh.Period_Name ÆÚ¼ä,
               Gjb.Name ÈÕ¼ÇÕÊÅúÃû,
               Gjh.Name ÈÕ¼ÇÕÊÃû,
               Gjl.Je_Line_Num ÈÕ¼ÇÕÊÐÐ,
               Gcc.Segment1 ¹«Ë¾±àÂë,
               (SELECT Fv1.Description
                  FROM Fnd_Flex_Values_Vl Fv1, Fnd_Flex_Value_Sets Fs1
                 WHERE Fv1.Flex_Value_Set_Id = Fs1.Flex_Value_Set_Id
                   AND Fs1.Flex_Value_Set_Name = 'QIHU_GL_COM'
                   AND Fv1.Flex_Value = Gcc.Segment1) ¹«Ë¾ËµÃ÷,
               Gcc.Segment5 ¿ÆÄ¿±àÂë,
               (SELECT Fv2.Description
                  FROM Fnd_Flex_Values_Vl Fv2, Fnd_Flex_Value_Sets Fs2
                 WHERE Fv2.Flex_Value_Set_Id = Fs2.Flex_Value_Set_Id
                   AND Fs2.Flex_Value_Set_Name = 'QIHU_GL_ACC'
                   AND Fv2.Flex_Value = Gcc.Segment5) ¿ÆÄ¿ËµÃ÷,
               Gcc.Segment6 ×ÓÄ¿±àÂë,
               (SELECT Fv3.Description
                  FROM Fnd_Flex_Values_Vl Fv3, Fnd_Flex_Value_Sets Fs3
                 WHERE Fv3.Flex_Value_Set_Id = Fs3.Flex_Value_Set_Id
                   AND Fs3.Flex_Value_Set_Name = 'QIHU_GL_SUBACC'
                   AND Fv3.Flex_Value = Gcc.Segment6) ×ÓÄ¿ËµÃ÷,
               Gcc.Segment9 ÍùÀ´±àÂë,
               (SELECT Fv4.Description
                  FROM Fnd_Flex_Values_Vl Fv4, Fnd_Flex_Value_Sets Fs4
                 WHERE Fv4.Flex_Value_Set_Id = Fs4.Flex_Value_Set_Id
                   AND Fs4.Flex_Value_Set_Name = 'QIHU_GL_INT'
                   AND Fv4.Flex_Value = Gcc.Segment9) ÍùÀ´ËµÃ÷,
               Gjh.Currency_Code ±ÒÖÖ,
               Nvl(Gjl.Entered_Dr, 0) Ô­±Ò½è·½,
               Nvl(Gjl.Entered_Cr, 0) Ô­±Ò´û·½,
               Nvl(Gjl.Accounted_Dr, 0) ±¾±Ò½è·½,
               Nvl(Gjl.Accounted_Cr, 0) ±¾±Ò´û·½,
               (Nvl(Gjl.Accounted_Dr, 0) - Nvl(Gjl.Accounted_Cr, 0)) ±¾±Ò½è·½¼õ´û·½,
               Gjsv.User_Je_Source_Name ÈÕ¼ÇÕËÀ´Ô´,
               Gjcv.User_Je_Category_Name ÈÕ¼ÇÕËÀà±ð,
               Decode(Gjh.Status, 'U', 'N', 'P', 'Y') ÊÇ·ñ¹ýÕË
          FROM Gl_Je_Batches            Gjb,
               Gl_Je_Headers            Gjh,
               Gl_Je_Lines              Gjl,
               Gl_Code_Combinations_Kfv Gcc,
               Gl_Ledgers               Gl,
               Gl_Je_Sources_Vl         Gjsv,
               Gl_Je_Categories_Vl      Gjcv
         WHERE Gjb.Je_Batch_Id = Gjh.Je_Batch_Id
           AND Gjh.Je_Header_Id = Gjl.Je_Header_Id
           AND Gjl.Code_Combination_Id = Gcc.Code_Combination_Id
           AND Gjh.Ledger_Id = Gl.Ledger_Id
           AND Gjsv.Je_Source_Name = Gjh.Je_Source
           AND Gjcv.Je_Category_Name = Gjh.Je_Category
           --AND Gjb.Creation_Date > Trunc(SYSDATE)
           )
 WHERE 1 = 1 --·ÖÀàÕÊ ='L_US_CNY'
   AND ÆÚ¼ä = '08-2015'
   AND ÈÕ¼ÇÕËÀ´Ô´ = 'TAC'
 ORDER BY 1, 2, 3, 4, 5␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
