              0,
              80) AS Access_Policy,
       Nvl2(Granteo.Instance_Set_Id,
            (SELECT Display_Name
               FROM Fnd_Object_Instance_Sets_Vl
              WHERE Instance_Set_Id = Granteo.Instance_Set_Id
                AND Object_Id = Granteo.Object_Id),
