       Nvl(SUM(Nvl(a.Acctd_Amount_Applied_To, a.Acctd_Amount_Applied_From)),
           0)
  FROM Ar_Receivable_Applications_All a
 WHERE a.Applied_Customer_Trx_Id IS NOT NULL
   AND Applied_Customer_Trx_Id <> -1␀ribution_Line_Number = D.Distribution_Line_Number - 1)
