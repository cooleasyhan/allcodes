tablespace APPS_TS_TX_DATA
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 128K
    next 128K
    minextents 1
    maxextents unlimited
    pctincrease 0
  )␀Blocking Other Processes ',
              2,
              ' 2 - THE LOCK IS NOT Blocking ANY Blocked Processes ON THE LOCAL Node,
       But It May OR May NOT Be Blocking Processes ON Remote Nodes. ')
  FROM V$lock          l,
       V$lock_Type     Lt,
       V$locked_Object Lo,
       All_Objects     Obj,
       V$session       s
 WHERE 1 = 1
   AND l.Type = Lt.Type
   AND Lo.Session_Id = s.Sid
   AND Lo.Object_Id = Obj.Object_Id
   AND l.Sid = s.Sid
   AND Obj.Object_Name = 'GL_JE_BATCHES'

   AND (l.Block <> 0 OR l.Request <> 0)␀   Acl.Attribute2       Uid_Number,
       Acl.Attribute3       Order_Number,
       Acl.Status_Dsp       Line_Status_Dsp,
       Acl.Claim_Amount,
       Acl.Claimant,
       Acl.Shelve_To_Flag,
       Ach.Attribute7       c_Account
  FROM Cux_Ar_Claim_Line_Us_b_v Acl, Cux_Ar_Claim_Header_Us_b_v Ach
 WHERE Acl.Header_Id(+) = Ach.Header_Id
   AND Ach.Status_Code <> '8'
