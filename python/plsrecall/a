PROD28␀ST01_PROD␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
drop synonym CUX_AR_CLAIM_ALL_TMP_S␀RY for cux.CUX_UPDATE_USER_HISTORY␀m_Type           VARCHAR2(240) := 'VALUE_SET_COA'; --$2;
  Lv_Application_Short_Name VARCHAR2(50) := 'DUMMY'; --$3;
  l_Objects                 Cux_Code_Move_Pub_Req.t_String_List;
BEGIN
  l_Objects := Cux_Code_Move_Pub_Req.Get_Objects_Name(Pin_Request_Id             => Ln_Request_Id,
                                                      Piv_Program_Type           => Lv_Program_Type,
                                                      Piv_Application_Short_Name => Lv_Application_Short_Name);

  IF l_Objects.Count < 1 THEN
    RAISE Fnd_Api.g_Exc_Error;
  END IF;
  Dbms_Output.Put_Line(l_Objects.Count());
  FOR i IN 1 .. l_Objects.Count LOOP
    dbms_output.put_line(i);
    IF i = 1 THEN
      Dbms_Output.Put_Line('OBJECTS:' || l_Objects(i));
    ELSE
      Dbms_Output.Put_Line(l_Objects(i));
    END IF;

    dbms_output.put_line(i);
  END LOOP;

EXCEPTION
  WHEN Fnd_Api.g_Exc_Error THEN
    Dbms_Output.Put_Line(Fnd_Api.g_Ret_Sts_Error || SQLERRM);
  WHEN OTHERS THEN
    Dbms_Output.Put_Line(Fnd_Api.g_Ret_Sts_Error || SQLERRM);
END;␀ounts         Sca,
       Hz_Parties               Sp,
       Cux_Ar_Customer_Balances_all Acb,
       Hz_Cust_Accounts         Hca,
       Hz_Parties               Hp,
       Hr_Operating_Units       Hou
 WHERE Sp.Party_Id(+) = Sca.Party_Id
   AND Sca.Cust_Account_Id(+) = Acb.Ship_To_Customer_Id
   AND Acb.Bill_To_Customer_Id = Hca.Cust_Account_Id(+)
   AND Hca.Party_Id = Hp.Party_Id(+)
   AND Acb.Org_Id = Hou.Organization_Id
   AND Acb.Set_Of_Books_Id = Nvl(2028, Acb.Set_Of_Books_Id)
   AND Acb.Set_Of_Books_Id = Hou.Set_Of_Books_Id
   AND Acb.Org_Id = Nvl(246, Acb.Org_Id)
   AND Decode('GL_DATE',
              'GL_DATE',
              Acb.Aging_Gl_Date,
              'TRX_DATE',
              Acb.Aging_Trx_Date) <= to_date('2015/04/30','yyyy-mm-dd')
   AND Acb.Account_Type = Nvl('AR', Acb.Account_Type) -- AR
