create synonym CUX_AR_CLAIM_ALL_TMP_S for cux.cux_ar_claim_temp_s␀L.ATTRIBUTE14
  FROM Ap_Invoices_All v, Hr_Operating_Units Ou, Ap_Batches_All a, AP_INVOICE_LINES_ALL L
 WHERE v.Created_By IN (1370, 1129) --('LIUYUJUAN','LVAIPING');
   AND Ou.Organization_Id = v.Org_Id
   AND Ou.Name LIKE '%PRC%'
   AND Ou.Name NOT LIKE '%QY%PRC%'
   AND v.Batch_Id = a.Batch_Id
   AND V.INVOICE_ID = L.INVOICE_ID␀ns Rptcnt VARCHAR2(10) Path '@RptCnt',
                        Rowcnt VARCHAR2(10) Path '@RowCnt',
                        Lincnt VARCHAR2(10) Path '@LinCnt',
                        Rptcells Xmltype Path 'syspfx_1:RptCell') Line,
               Xmltable(Xmlnamespaces('http://www.oracle.com/fsg/2002-03-20/' AS
                                      "syspfx_1"),
                        '/syspfx_1:RptCell' Passing Line.Rptcells Columns
                        Colcnt VARCHAR2(10) Path '@ColCnt',
                        VALUE VARCHAR2(32) Path 'text()') Cell) Rst
pivot
(
max(value) for colcnt in ('c1000','c1001','c1002','c1003',
'c1004','c1005','c1006','c1007','c1008','c1009','c1010',
'c1011','c1012','c1013','c1014','c1015','c1016')
)␀ated_Group, Xe_Created_Group), Gjl_Created_Group) ËùÊôteam,
       Event_Id ÊÂ¼þ±àºÅ,
       Event_Type_Name ÊÂ¼þÀàÐÍ,
       Nvl(Xal_Entered_Dr, Gjl_Entered_Dr) Ô­±Ò½è·½½ð¶î,
       Nvl(Xal_Entered_Cr, Gjl_Entered_Cr) Ô­±Ò´û·½½ð¶î,
       Nvl(Xal_Accounted_Dr, Gjl_Accounted_Dr) ±¾±Ò½è·½½ð¶î,
       Nvl(Xal_Accounted_Cr, Gjl_Accounted_Cr) ±¾±Ò´û·½½ð¶î, Cgts.Je_Header_Id, cgts.ae_header_id, cgts.trx_number, cgts.trx_type
  FROM Cux_Gl_To_Sub_Ids_All Cgts
 WHERE 1 = 1
   AND Ledger_Id IN (SELECT s.Set_Of_Books_Id
                       FROM Gl_Sets_Of_Books s
                      WHERE s.Name IN ('L_QY_US_CNY',
                                       'L_QY_US_USD',
                                       'L_US_CNY',
                                       'L_US_JPY',
                                       'L_US_USD',
                                       'L_US_TWD'))
   AND Effective_Date BETWEEN
       (SELECT Start_Date
          FROM Gl_Period_Statuses Gps
         WHERE Gps.Application_Id = 101
           AND Gps.Ledger_Id = Cgts.Ledger_Id
           AND Gps.Period_Name = Nvl('', Cgts.Period_Name))
   AND (SELECT End_Date
          FROM Gl_Period_Statuses Gps
         WHERE Gps.Application_Id = 101
           AND Gps.Ledger_Id = Cgts.Ledger_Id
           AND Gps.Period_Name = Nvl('07-2015', Cgts.Period_Name))␀g = 'N'
      -- AND Gp.Period_Name = Piv_Period_Name
   AND Aip.Accounting_Date BETWEEN Gp.Start_Date AND Gp.End_Date␀                   '9',
