  FROM V$lock          l,
       V$lock_Type     Lt,
       V$locked_Object Lo,
       All_Objects     Obj,
       V$session       s
 WHERE 1 = 1
   AND l.Type = Lt.Type
   AND Lo.Session_Id = s.Sid
   AND Lo.Object_Id = Obj.Object_Id
   AND l.Sid = s.Sid
