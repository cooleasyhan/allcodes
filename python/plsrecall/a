select * from dba_objects t where t.OBJECT_NAME like '%ASSIG%'␀ Str_2_Tab
  FROM Dual
CONNECT BY LEVEL <= Length('Err1;Err2;Err3') -
           Length(REPLACE('Err1;Err2;Err3', ';', '')) + 1␀number(c.orig_system_reference);
         exception
           when others then null;
         end;
