create table Ra_Cust_Trx_Line_Gl_Dist_1010 as select * from Ra_Cust_Trx_Line_Gl_Dist_All␀lue_Sets Ffvs, Fnd_Flex_Values_Tl Tl
 WHERE Ffv.Flex_Value_Set_Id = Ffvs.Flex_Value_Set_Id
   AND Tl.Flex_Value_Id = Ffv.Flex_Value_Id
   AND Ffvs.Flex_Value_Set_Name LIKE 'QIHU%'
   AND (Ffv.Last_Update_Date >= SYSDATE -1 OR
