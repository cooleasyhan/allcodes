select * from gl_import_references t where t.gl_sl_link_table like 'CUX_A%'␀me like 'CUX%'␀[FILENAME]] -o '||APP_FTP_BASEPATH||'[[FILENAME]]"'  FROM CUX_UNIFY_IMPORT_APPLICATION␀quest_Id IS NULL
 group by  Program_Type, Application_Short_Name␀Object_Name,
                             SCHEMA      => t.Owner) Ddl
  FROM Dba_Objects t
 WHERE t.Object_Name LIKE 'CUX%'
   AND t.Object_Type IN ('TABLE', 'VIEW', 'PACKAGE', 'TRIGGER', 'SEQUENCE')
   and t.OBJECT_NAME = 'CUXADMINR_IMP_PKG'
