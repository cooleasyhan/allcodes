SELECT DISTINCT u.Security_Id_Int_1, u.Entity_Code, ou.name
  FROM Xla_Events                   e,
       Xla_Transaction_Entities_Upg u,
       Cux_Update_User_History      h, hr_operating_units ou
 WHERE e.Entity_Id = u.Entity_Id
   AND h.Object_Table = 'XLA_EVENTS'
   AND h.Object_Id = e.Event_Id
   and ou.organization_id = u.security_id_int_1␀ UNION ALL
              SELECT DISTINCT /*Ou.Name,
                              v.Batch_Id,
                              a.Batch_Name,*/ l.Attribute13
                FROM Ap_Invoices_All      v,
                     Hr_Operating_Units   Ou,
                     Ap_Batches_All       a,
                     Ap_Invoice_Lines_All l
               WHERE v.Created_By IN (1370, 1129) --('LIUYUJUAN','LVAIPING');
                 AND Ou.Organization_Id = v.Org_Id
                 AND Ou.Name LIKE '%PRC%'
                 AND Ou.Name NOT LIKE '%QY%PRC%'
                 AND v.Batch_Id = a.Batch_Id
                 AND v.Invoice_Id = l.Invoice_Id␀         WHERE HZ.CUST_ACCOUNT_ID =
                                       V.BILL_TO_CUSTOMER_ID) = 'N' THEN
                           0
                          ELSE
                           QUANTITY
                        END QUANTITY,
                        V.CUSTOMER_TRX_ID,
                        V.CUSTOMER_TRX_LINE_ID
          FROM CUX_AR_CUSTOMER_TRXS_B_V V
         WHERE V.OU_NAME LIKE '%²ÊÆ±%US%'
           AND ACCOUNT_CLASS = 'REV'
           AND LINE_TYPE = 'LINE'
           AND NVL(V.CTT_NAME, 'DUMMY') <> '²ÊÃñ½±Àø'
           AND (V.QUANTITY <> 0 OR V.EXTENDED_AMOUNT <> 0)
           AND V.GL_DATE BETWEEN TO_DATE('01-2015', 'MM-YYYY') AND
               LAST_DAY(TO_DATE('01-2015', 'MM-YYYY'))
           AND V.COMPLETE_FLAG = 'Y'
        --     AND Mo_Global.Check_Access(v.Org_Id) = 'Y'
        /*       AND EXISTS
        (SELECT 1
                 FROM Hz_Cust_Accounts Hz
                WHERE Hz.Cust_Account_Id = v.Bill_To_Customer_Id
                  AND Hz.Attribute3 = 'Y')*/
        ) RST --Á÷Ë®Í³¼Æ ±ê¼ÇÊÇ·ñÎªY
WHERE SHIP_TO_CUSTOMER_NAME = 'É½¶«Ê¡ÌåÓý²ÊÆ±¹ÜÀíÖÐÐÄ_²ÊÆ±'
