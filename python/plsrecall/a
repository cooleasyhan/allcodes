select * from icx_sessions␀_Name, Bs_Description, v.Trx_Attr12
  FROM Cux_Ar_Customer_Trxs_b_v v
 WHERE v.Ledger_Id IN
       (SELECT Ledger_Id
          FROM Gl_Ledgers Gl
         WHERE Gl.Name LIKE '%\_PRC\_%' ESCAPE
         '\'
           AND Gl.Name NOT LIKE '%QY%\_PRC\_%' ESCAPE '\')
   AND v.Created_By IN (1370, 1129)␀IPING');
