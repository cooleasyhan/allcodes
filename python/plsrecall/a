select * from cux_ar_claim_prc_b_v v where v.Bank_Receipt_Num like '%AD09%' order by v.Creation_Date desc␀IN (1370, 1129)
   AND Ou.Organization_Id = v.Org_Id
   AND Ou.Name LIKE '%PRC%'␀butions_All D2
         WHERE D2.Invoice_Id = d.Invoice_Id
           AND D2.Invoice_Line_Number = d.Invoice_Line_Number
           AND d2.Distribution_Line_Number = D.DISTRIBUTION_LINE_NUMBER - 1) new_created_by
  FROM Ap_Invoice_Distributions_All d
 WHERE d.Description = '430BURECLASS'
 and d.amount <> 0
 and d.line_type_lookup_code = 'ITEM'
 AND EXISTS (SELECT 1 FROM AP_INVOICES_ALL A
 WHERE A.INVOICE_NUM NOT LIKE '%NEW'
 AND A.INVOICE_ID = D.INVOICE_ID
 and a.invoice_num not like '%ADJ'
 and a.invoice_num not like '%¹ÖÎïÁÔÈË´óá÷ÁÔ¶à·½·Ö³É%'
 and a.invoice_id not in (38307))␀
       Cr.Attribute7                 Acr_Attr7,
       Cr.Attribute8                 Acr_Attr8,
       Cr.Attribute9                 Acr_Attr9,
       Cr.Attribute10                Acr_Attr10,
       Cr.Confirmed_Flag,
       Cr.Customer_Site_Use_Id,
       Cr.Deposit_Date,
       Cr.Receipt_Method_Id,
       Cr.Customer_Receipt_Reference,
       Cr.Org_Id,
       Cr.Unique_Reference,
       Cr.Remittance_Bank_Account_Id,
       Cr.Remit_Bank_Acct_Use_Id,
       Cr.Legal_Entity_Id
  FROM Ar_Cash_Receipts_All        Cr,
       Ar_Cash_Receipt_History_All Ach,
       Hz_Cust_Accounts            Cust,
       Hz_Parties                  Party,
       Hz_Cust_Site_Uses_All       Site_Uses
 WHERE Cr.Cash_Receipt_Id = Ach.Cash_Receipt_Id(+)
   AND Cr.Org_Id = Ach.Org_Id(+)
   AND Ach.First_Posted_Record_Flag(+) = 'Y'
   AND Cr.Pay_From_Customer = Cust.Cust_Account_Id(+)
   AND Cust.Party_Id = Party.Party_Id(+)
   AND Cr.Customer_Site_Use_Id = Site_Uses.Site_Use_Id(+)
   AND Cr.Org_Id = Site_Uses.Org_Id(+)␀') AND
                     Last_Day(To_Date('10-2014', 'mm-yyyy'))
                 AND Ach.Receipt_Date BETWEEN
                     Nvl(Fnd_Date.Canonical_To_Date(''),
                         Ach.Receipt_Date -1) AND
                     Nvl(Fnd_Date.Canonical_To_Date(''),
                         Ach.Receipt_Date + 1)
                 AND Ach.Company_Code =
                     Nvl(NULL, Ach.Company_Code)
               ORDER BY "¹«Ë¾±àÂë",
                        "ÒøÐÐÕË»§",
                        "¸¶¿î·½",
                        "ÊÕ¿îÈÕÆÚ",
                        "ÒøÐÐ»Øµ¥ºÅ") Rst␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
