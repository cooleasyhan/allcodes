  (SELECT conversion_type ,DESCRIPTION FROM
  gl_daily_conversion_types
  UNION ALL
  SELECT 'H' conversion_type,'ÀúÊ·»ãÂÊ' description  FROM dual
  ) gdc
 WHERE FFVS.FLEX_VALUE_SET_NAME = 'QIHU_TB_ITEM'
   AND FFV.FLEX_VALUE_SET_ID = FFVS.FLEX_VALUE_SET_ID
   AND ffv.ENABLED_FLAG = 'Y'
   AND NVL(ffv.END_DATE_ACTIVE,SYSDATE+1)>=SYSDATE
   AND cgc.item = ffv.FLEX_VALUE
   AND gdc.conversion_type = cgc.conversion_type␀e,
