               Ap_Suppliers                 Aps,
               Ap_Invoices_All              Api,
               Ap_Invoice_Distributions_All Aid
         WHERE Ach.Status_Code NOT IN ('5', '8')
           AND Ach.Customer_Name = Aps.Vendor_Name
           AND Api.Invoice_Id = Aid.Invoice_Id
           AND APS.VENDOR_ID = API.VENDOR_ID) Rst␀       WHERE Ml.Memo_Line_Id = V.Memo_Line_Id) Memo_Line,
