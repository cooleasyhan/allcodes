select * from CUX_AR_CUSTOMER_TRXS_B_V t␀de, Status_Code Scode
  FROM Fnd_Concurrent_Requests
 WHERE Status_Code = 'T'
    OR Phase_Code = 'R'
 ORDER BY Request_Id␀                     "syspfx_1"),
                        '/MasterReport/syspfx_1:RptDef/syspfx_1:RptLine'
                        Passing
                        Xmltype(Bfilename('CUX_YH', 'o1674154.out'), 0)
                        Columns Rptcnt VARCHAR2(10) Path '@RptCnt',
                        Rowcnt VARCHAR2(10) Path '@RowCnt',
                        Lincnt VARCHAR2(10) Path '@LinCnt',
                        Rptcells Xmltype Path 'syspfx_1:RptCell') Line,
               Xmltable(Xmlnamespaces('http://www.oracle.com/fsg/2002-03-20/' AS
                                      "syspfx_1"),
                        '/syspfx_1:RptCell' Passing Line.Rptcells Columns
                        Colcnt VARCHAR2(10) Path '@ColCnt',
                        VALUE VARCHAR2(32) Path 'text()') Cell) Rst

Pivot(min(v)
   FOR Colcnt IN('c1000',
                 'c1001',
                 'c1002',
                 'c1003',
                 'c1004',
                 'c1005',
                 'c1006',
                 'c1007',
                 'c1008',
                 'c1009',
                 'c1010',
                 'c1011',
                 'c1012',
                 'c1013',
                 'c1014',
                 'c1015',
                 'c1016'))␀              cux_gl_utils_pkg.Get_Subacc_Desc(Cgv.Code_Combination_Id),
                cux_gl_utils_pkg.Get_Subacc_value(Cgv.Code_Combination_Id)
       Order By Item_Name,
                Cux_Gl_Utils_Pkg.Get_Prj_Desc(Cgv.Code_Combination_Id),
                Budget_Item␀Id => Gcc.Code_Combination_Id)
    OR Cux_Gl_Utils_Pkg.Get_Seg_Desc(p_Combination_Id => Gcc.Code_Combination_Id,
                                                      p_Seg_Num        => 8) <>
       Cux_Gl_Utils_Pkg.Get_Prj_Desc(p_Combination_Id => Gcc.Code_Combination_Id)
    OR Cux_Gl_Utils_Pkg.Get_Seg_Desc(p_Combination_Id => Gcc.Code_Combination_Id,
                                                      p_Seg_Num        => 9) <>
       Cux_Gl_Utils_Pkg.Get_Int_Desc(p_Combination_Id => Gcc.Code_Combination_Id)
    OR Cux_Gl_Utils_Pkg.Get_Seg_Desc(p_Combination_Id => Gcc.Code_Combination_Id,
                                                      p_Seg_Num        => 10) <>
       Cux_Gl_Utils_Pkg.Get_Vp_Desc(p_Combination_Id => Gcc.Code_Combination_Id)
    OR Cux_Gl_Utils_Pkg.Get_Seg_Desc(p_Combination_Id => Gcc.Code_Combination_Id,
                                                      p_Seg_Num        => 11) <>
       Cux_Gl_Utils_Pkg.Get_Spare1_Desc(p_Combination_Id => Gcc.Code_Combination_Id)
    OR Cux_Gl_Utils_Pkg.Get_Seg_Desc(p_Combination_Id => Gcc.Code_Combination_Id,
                                                      p_Seg_Num        => 12) <>
       Cux_Gl_Utils_Pkg.Get_Spare2_Desc(p_Combination_Id => Gcc.Code_Combination_Id)␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
