select distinct status from Ar_Receivable_Applications_All a where a.applied_customer_trx_id is not null␀rn_status => s,
                                      x_msg_data =>s,
                                      p_commit => 't',
                                      p_interface_run_id => 79443);
end;␀
  creation_date          DATE,
  created_by             NUMBER,
  last_update_date       DATE,
  last_updated_by        NUMBER,
  last_update_login      NUMBER,
  download_request_id    NUMBER
)␀gment1) CAT_MIN,
       Gt.Ori_Cost,
       Cost,
       Date_Placed_In_Service,
       Life_In_Month,
       Cal_Life_Remain,
       Nvl(Ytd_Deprn_Amount, 0) Ytd_Deprn_Amount,
       Nvl(Acct_Deprn_Amount, 0) Acct_Deprn_Amount,
       Cal_Book_Net_Value,
       Com_Desc,
       Expense_Account,
       Expense_Account_Desc
  FROM Cux_Fa_Asset_Main_Gt Gt␀'),
              -8,
              Arpt_Sql_Func_Util.Get_Lookup_Meaning('ACTIVITY_APPS',
                                                    'REFUND'),
              -9,
              Arpt_Sql_Func_Util.Get_Lookup_Meaning('ACTIVITY_APPS',
                                                    'CC_CHARGEBACK'),
              Rct.Trx_Number) Trx_Number,
       App.Amount_Applied,
       Nvl(App.Amount_Applied_From, App.Amount_Applied) Amount_Applied_From,
       App.Acctd_Amount_Applied_From,
       App.Acctd_Amount_Applied_To,
       To_Number(Decode(Sign(App.Applied_Payment_Schedule_Id),
                        -1,
                        NULL,
                        App.Acctd_Amount_Applied_From -
                        Nvl(App.Acctd_Amount_Applied_To,
                            App.Acctd_Amount_Applied_From))) Exchange_Gain_Loss,

       App.Gl_Posted_Date,
       App.Set_Of_Books_Id,
       App.Display,
       App.Application_Type,
       App.Payment_Schedule_Id,
       App.Posting_Control_Id,
       App.Applied_Customer_Trx_Id,
       App.Applied_Customer_Trx_Line_Id
  FROM Ar_Receivable_Applications_All App,
       Ar_Cash_Receipts_All           Cr,
       Ra_Customer_Trx_All            Rct
 WHERE App.Cash_Receipt_Id = Cr.Cash_Receipt_Id
   AND App.Applied_Customer_Trx_Id = Rct.Customer_Trx_Id(+)␀_Id,
                                     v.Code_Combination_Id) Proj_Desc,
       --V.Ledger_Id,
       Decode(v.Account_Class,
              'REV',
              CASE
                WHEN v.Short_Code LIKE '1010%' THEN
                 To_Char(v.Quantity)
                ELSE
                 v.Ctl_Attr1
              END,
              '') Line_Dff_Amount,
       Decode(v.Account_Class,
              'REV',
              CASE
                WHEN v.Short_Code LIKE '1010%' THEN
                 To_Char(v.Unit_Selling_Price)
                ELSE
                 v.Ctl_Attr2
              END,
              '') Percentage,
       v.Amount,
       v.Comments,
       Fu.User_Name Created_By,
       v.Ou_Name,
       v.Short_Code, v.Customer_Trx_Id
  FROM Cux_Ar_Customer_Trxs_b_v v,
       Gl_Code_Combinations_Kfv Gcc,
       Fnd_User                 Fu
 WHERE v.Code_Combination_Id = Gcc.Code_Combination_Id
   AND v.Created_By = Fu.User_Id(+) --AND V.Account_Class <> 'REC' --ÏÔÊ¾Ó¦ÊÕÕË¿î

   AND v.Ou_Name LIKE '%²ÊÆ±%'
 ORDER BY Ou_Name, Trx_Number, Line_Number, Account_Class␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
