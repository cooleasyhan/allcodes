create table ap_invoice_lines_1010 as select * from ap_invoice_lines_all␀_Set_Name)  Flex_Value_Set_Name
  FROM (SELECT DISTINCT Ffvs.Flex_Value_Set_Name
          FROM Fnd_Flex_Values     Ffv,
               Fnd_Flex_Value_Sets Ffvs,
               Fnd_Flex_Values_Tl  Tl
         WHERE Ffv.Flex_Value_Set_Id = Ffvs.Flex_Value_Set_Id
           AND Tl.Flex_Value_Id = Ffv.Flex_Value_Id
           AND Ffvs.Flex_Value_Set_Name LIKE 'QIHU%'
           AND (Ffv.Last_Update_Date >= SYSDATE - 1 OR
               Ffvs.Last_Update_Date >= SYSDATE - 1 OR
               Tl.Last_Update_Date >= SYSDATE - 1)) Rst␀  creation_date               DATE,
  created_by                  NUMBER(15),
  last_update_login           NUMBER(15)
)␀2(1000),
