select t.cust_trx_type_id

          from ra_cust_trx_types_all t
         where 1=1
           and org_id=264
           and name='ZYÁªÔË_CPS_ÓÐ'␀ble_flag varchar2(1))␀date('2015/4/27 16:00:00','yyyy/mm/dd hh24:mi:ss')␀'yyyy-mm-dd')␀lity_Id
   AND Re.Responsibility_Application_Id = Vl.Application_Id
   and fu.user_id = re.REQUESTED_BY
   AND Vl.Responsibility_Name = 'Ó¦ÊÕ²éÑ¯_US'␀ild_Flex_Value_Low AND
       Vh.Child_Flex_Value_High
   AND Ffv.Flex_Value = Vh.Parent_Flex_Value␀aning('CHECK_STATUS', Cr.Status) Receipt_Status_Dsp,
       Cr.Type,
       Cr.Receipt_Number,
       Cr.Receipt_Date,
       Cr.Exchange_Rate,
       Cr.Exchange_Date,
       Cr.Exchange_Rate_Type,
       Cr.Attribute_Category Acr_Attr_Category,
       Cr.Attribute1 Acr_Attr1,
       Cr.Attribute2 Acr_Attr2,
       Cr.Attribute3 Acr_Attr3,
       Cr.Attribute4 Acr_Attr4,
       Cr.Attribute5 Acr_Attr5,
       Cr.Attribute6 Acr_Attr6,
       Cr.Attribute7 Acr_Attr7,
       Cr.Attribute8 Acr_Attr8,
       Cr.Attribute9 Acr_Attr9,
       Cr.Attribute10 Acr_Attr10,
       Cr.Confirmed_Flag,
       Cr.Customer_Site_Use_Id,
       Cr.Deposit_Date,
       Cr.Receipt_Method_Id,
       Rc.Name Receipt_Class_Dsp,
       Arpt_Sql_Func_Util.Get_Lookup_Meaning('RECEIPT_CREATION_METHOD',
                                             Rc.Creation_Method_Code) Creation_Method_Dsp,
       Rc.Creation_Method_Code Creation_Method_Code,
       Rec_Method.Name Receipt_Method_Dsp,
       Rec_Method.Payment_Channel_Code Payment_Type_Code,
       Ce_Bank_And_Account_Util.Get_Masked_Bank_Acct_Num(Remit_Bank.Bank_Account_Id) Remit_Bank_Account,
       Cba.Currency_Code Remit_Bank_Currency,
       Bb.Bank_Name Remit_Bank_Name,
       Bb.Branch_Party_Id Remittance_Bank_Branch_Id,
       Bb.Bank_Branch_Name Remit_Bank_Branch,
       Cr.Customer_Receipt_Reference,
       Cr.Org_Id,
       Ou.Name Org_Name,
       Cr.Unique_Reference,
       Cr.Remittance_Bank_Account_Id,
       Cr.Remit_Bank_Acct_Use_Id,
       Cr.Legal_Entity_Id
  FROM Ar_Cash_Receipts_All        Cr,
       Ar_Cash_Receipt_History_All Ach,
       Hz_Cust_Accounts            Cust,
       Hz_Parties                  Party,
       Hz_Cust_Site_Uses_All       Site_Uses,
       Ce_Bank_Accounts            Cba,
       Ce_Bank_Acct_Uses_All       Remit_Bank,
       Ce_Bank_Branches_v          Bb,
       Ar_Receipt_Methods          Rec_Method,
       Ar_Receipt_Classes          Rc,
       Hr_Operating_Units          Ou
 WHERE Cr.Cash_Receipt_Id = Ach.Cash_Receipt_Id(+)
   AND Cr.Org_Id = Ach.Org_Id(+)
   AND Ach.First_Posted_Record_Flag(+) = 'Y'
   AND Cr.Pay_From_Customer = Cust.Cust_Account_Id(+)
   AND Cust.Party_Id = Party.Party_Id(+)
   AND Cr.Customer_Site_Use_Id = Site_Uses.Site_Use_Id(+)
   AND Cr.Org_Id = Site_Uses.Org_Id(+)
   AND Remit_Bank.Bank_Acct_Use_Id(+) = Cr.Remit_Bank_Acct_Use_Id
   AND Remit_Bank.Org_Id(+) = Cr.Org_Id
   AND Remit_Bank.Bank_Account_Id = Cba.Bank_Account_Id(+)
   AND Bb.Branch_Party_Id(+) = Cba.Bank_Branch_Id
   AND Cr.Receipt_Method_Id = Rec_Method.Receipt_Method_Id
   AND Rec_Method.Receipt_Class_Id = Rc.Receipt_Class_Id
   AND Cr.Org_Id = Ou.Organization_Id␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
