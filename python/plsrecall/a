select distinct parameters from fnd_form_functions␀interface_run_id = 98271 for update␀l
CONNECT BY LEVEL <= Length('Err1, Err2, Err3') -
           Length(REPLACE('Err1, Err2, Err3', ',', '')) + 1␀voice_Id
           AND D2.Invoice_Line_Number = d.Invoice_Line_Number
           AND d2.Distribution_Line_Number = D.DISTRIBUTION_LINE_NUMBER - 1)
  FROM Ap_Invoice_Distributions_All d
 WHERE d.Description = '430BURECLASS'
 AND EXISTS (SELECT 1 FROM AP_INVOICES_ALL A
 WHERE A.INVOICE_NUM NOT LIKE '%NEW'
 AND A.INVOICE_ID = D.INVOICE_ID)␀      p_Upload_Type   => 'DB');
  Dbms_Output.Put_Line(Lv_Return_Status);
  Dbms_Output.Put_Line(Lv_Msg_Data);
  Dbms_Output.Put_Line(LN_INTERFACE_RUN_ID);
END;␀UTE2 ×Ö¶ÎUID,
       cacl.ATTRIBUTE3 ¶©µ¥ºÅ,
       cacl.CLAIMANT ÐÐÈÏÁìÈË,
       cach.ATTRIBUTE6 Í·ÉóÅú×´Ì¬,
       cach.attribute3 Í·´«ËÍGL,
       cacl.attribute14 CRM·µ»ØID
  from cux_ar_claim_header_us_all cach,
       cux_ar_claim_line_us_all  cacl,
       hr_operating_units hou,
       fnd_lookup_values_vl flv,
       fnd_lookup_values_vl flv2,
       fnd_lookup_values_vl flv3,
       hz_parties hp,
       gl_ledgers gl
 where cach.header_id = cacl.header_id
   and cacl.org_id = hou.organization_id(+)
   and cacl.us_customer_id = hp.party_id (+)
   AND flv3.lookup_type = 'CUX_RECEIPT_CATEGORY'
   AND flv3.lookup_code = cacl.receipt_category
   AND flv2.lookup_type = 'CUX_RECEIPT_HEADER_STATUS'
   AND flv2.lookup_code = cach.status_code
   AND flv.lookup_type = 'CUX_RECEIPT_LINE_STATUS'
   AND flv.lookup_code = cacl.status_code
   AND cacl.ledger_id = gl.ledger_id(+)
   AND flv.enabled_flag = 'Y'
   and cach.header_id in (48614,
                                              48620,
