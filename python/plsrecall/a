ATTRIBUTE20
 from fnd_flex_values f  where f.COMPILED_VALUE_ATTRIBUTES is not null␀¼ÆËã', '130', '130', null, '1', 'Y', 'Y', null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, 3, 1124, to_date('04-06-2015 13:31:38', 'dd-mm-yyyy hh24:mi:ss'), 1124, to_date('04-06-2015 13:38:13', 'dd-mm-yyyy hh24:mi:ss'), 624022)␀_Code,
       Xdl.Applied_To_Source_Id_Num_1,
       Xdl.Applied_To_Distribution_Type,
       Xdl.Applied_To_Dist_Id_Num_1
  FROM Ar_Receivable_Applications_All Ara,
       Ar_Distributions_All           Ada,
       Ar_Cash_Receipts_All           Acr,
       Ra_Customer_Trx_All            Rct,
       Ra_Customer_Trx_All            Cm,
       Xla_Distribution_Links         PARTITION(Ar) Xdl,
       Xla_Ae_Headers                 PARTITION(Ar) Xah,
       Hr_Operating_Units             Hou
 WHERE 1 = 1
   AND Ara.Gl_Posted_Date IS NULL
   AND Ara.Gl_Date BETWEEN To_Date('2015-05-01', 'yyyy-mm-dd') AND
       To_Date('2015-05-31', 'yyyy-mm-dd') + 0.99999
   AND Ada.Source_Table = 'RA'
   AND Ada.Source_Id = Ara.Receivable_Application_Id
   AND Acr.Cash_Receipt_Id(+) = Ara.Cash_Receipt_Id
   AND Rct.Customer_Trx_Id(+) = Ara.Applied_Customer_Trx_Id
   AND Cm.Customer_Trx_Id(+) = Ara.Customer_Trx_Id
   AND Xdl.Source_Distribution_Type(+) = 'AR_DISTRIBUTIONS_ALL'
   AND Xdl.Source_Distribution_Id_Num_1(+) = Ada.Line_Id
   AND Xah.Ae_Header_Id(+) = Xdl.Ae_Header_Id
   AND Hou.Organization_Id = Ara.Org_Id
   AND Hou.Name LIKE '%PRC%'
 ORDER BY 1, 2, 3, 4, 5␀ild_Flex_Value_High,
                  (Decode(s.Maximum_Size,
                          5,
                          'HH24:MI',
                          8,
                          'HH24:MI:SS',
                          9,
                          'DD-MON-RR',
                          11,
                          'DD-MON-YYYY',
                          15,
                          'DD-MON-RR HH24:MI',
                          17,
                          'DD-MON-YYYY HH24:MI',
                          18,
                          'DD-MON-RR HH24:MI:SS',
                          20,
                          'DD-MON-YYYY HH24:MI:SS'))))) OR
       ((s.Format_Type NOT IN ('N', 'D', 'T')) AND
       (v.Flex_Value BETWEEN h.Child_Flex_Value_Low AND
       h.Child_Flex_Value_High)))
   AND ((v.Summary_Flag = 'Y' AND h.Range_Attribute = 'P') OR
       (v.Summary_Flag = 'N' AND h.Range_Attribute = 'C'))
       and h.flex_value_set_id = 1014993␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
