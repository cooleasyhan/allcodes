       Attribute50,
       Attribute_Sort_Order,
       Row_Id,HIERARCHY_LEVEL
  FROM Fnd_Flex_Values_Vl
 WHERE (('' IS NULL) OR
       (Structured_Hierarchy_Level IN
       (SELECT Hierarchy_Id
            FROM Fnd_Flex_Hierarchies_Vl h
           WHERE h.Flex_Value_Set_Id = 1015793
             AND h.Hierarchy_Name LIKE '')))
   AND (Flex_Value_Set_Id = 1015793)
 ORDER BY Flex_Value␀
              4,
              ' 4 - SHARE(s) ',
              5,
              ' 5 - s / ROW - x(Ssx) ',
              6,
              ' 6 - EXCLUSIVE(x) ') Request,
       l.Block,
       Decode(l.Block,
              0,
              ' 0 - THE LOCK IS NOT Blocking ANY Other Processes ',
              1,
              ' 1 - THE LOCK IS Blocking Other Processes ',
              2,
              ' 2 - THE LOCK IS NOT Blocking ANY Blocked Processes ON THE LOCAL Node,
       But It May OR May NOT Be Blocking Processes ON Remote Nodes. ')
  FROM V$lock          l,
       V$lock_Type     Lt,
       V$locked_Object Lo,
       All_Objects     Obj,
       V$session       s
 WHERE 1 = 1
   AND l.Type = Lt.Type
   AND Lo.Session_Id = s.Sid
   AND Lo.Object_Id = Obj.Object_Id
   AND l.Sid = s.Sid
  AND Obj.Object_Name LIKE 'FA_DISTRIBUTION_HISTORY%'
   AND (l.Block <> 0 OR l.Request <> 0)␀AND Ail.Invoice_Id = Api.Invoice_Id
