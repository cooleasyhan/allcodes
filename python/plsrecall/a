SELECT DISTINCT Ou_Name, Bs_Name, Bs_Description, v.Trx_Attr12
  FROM Cux_Ar_Customer_Trxs_b_v v
 WHERE   v.ou_Name LIKE '%PRC%'
   AND v.ou_Name NOT LIKE '%QY%PRC%'
   AND v.Created_By IN (1370, 1129)␀            New_Name VARCHAR2(20),
                                     New_Description VARCHAR2(80))␀o.Net p,
       Cux_Unify_Import_Lay_Map@Demo2.Ebs.Corp.Qihoo.Net l,
       Cux_Unify_Import_Prf_Req@Demo2.Ebs.Corp.Qihoo.Net r
 WHERE p.Layout_Id = l.Layout_Id
   AND p.Interface_Profile_Id = r.Interface_Profile_Id
   AND Interface_Key = 'S_HEC_GL'
MINUS
SELECT Interface_Key,
       Interface_Desc,
       Col_Seq,
       Col_Type,
       Table_Col,
       Col_Name,
       Col_Desc,
       Iface_Col,
       File_Col,
       Col_Length,
       Data_Format,
       Enabled_Flag,
       Display_Flag
  FROM Cux_Unify_Import_Profile p,
       Cux_Unify_Import_Lay_Map l,
       Cux_Unify_Import_Prf_Req r
 WHERE p.Layout_Id = l.Layout_Id
   AND Interface_Key = 'S_HEC_GL'
   AND p.Interface_Profile_Id = r.Interface_Profile_Id␀            Acb.Reference)) Diff_Days,
       Acb.Source_Created_By
  FROM Hz_Cust_Accounts         Sca,
       Hz_Parties               Sp,
       Cux_Ar_Customer_Balances_all Acb,
       Hz_Cust_Accounts         Hca,
       Hz_Parties               Hp,
       Hr_Operating_Units       Hou
 WHERE Sp.Party_Id(+) = Sca.Party_Id
   AND Sca.Cust_Account_Id(+) = Acb.Ship_To_Customer_Id
   AND Acb.Bill_To_Customer_Id = Hca.Cust_Account_Id(+)
   AND Hca.Party_Id = Hp.Party_Id(+)
   AND Acb.Org_Id = Hou.Organization_Id
   AND Acb.Set_Of_Books_Id = Nvl(2028, Acb.Set_Of_Books_Id)
   AND Acb.Set_Of_Books_Id = Hou.Set_Of_Books_Id
   AND Acb.Org_Id = Nvl(246, Acb.Org_Id)
   AND Decode('GL_DATE',
              'GL_DATE',
              Acb.Aging_Gl_Date,
              'TRX_DATE',
              Acb.Aging_Trx_Date) <= to_date('2015/04/30','yyyy-mm-dd')
   AND Acb.Account_Type = Nvl('AR', Acb.Account_Type) -- AR
   AND 'Y' = (CASE
                WHEN 'AR' <> 'RA' THEN
                 'Y'
                WHEN 'AR' = 'RA' AND 'ACC' IS NULL THEN
                 'Y'
                WHEN 'AR' = 'RA' AND 'ACC' = 'ACC' AND
                     Acb.Ra_Status = 'ACC' THEN
                 'Y'
                WHEN 'AR' = 'RA' AND 'ACC' = 'UN_ACC' AND
                     Acb.Ra_Status <> 'ACC' THEN
                 'Y'
                ELSE
                 'N'
              END)
   AND Acb.Account_Code BETWEEN Nvl('', Acb.Account_Code) AND
       Nvl('', Acb.Account_Code)
      /* AND Cux_Ar_Customer_Balance_Pkg.Check_By_Tb(&p_Is_Submit_By_Tb,
                                                   'EXCLUDE_ACCRUE',
                                                   Acb.Account_Code) = 'Y'
      */

   AND (Acb.Bill_To_Customer_Id = '' OR
        '' IS NULL)
   AND (Acb.Contact_Number = '' OR '' IS NULL)␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
