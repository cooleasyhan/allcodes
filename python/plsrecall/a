select v.MEANING
      from fnd_lookup_values_vl v
     where v.LOOKUP_TYPE = decode('A02'
                                 ,'A01','CUX_PG_ID_TRXID'
                                 ,'A02',decode((select short_code from hr_operating_units where organization_id=264)
                                              ,'101803','CUX_MGAD_ID_TRXID'
                                              ,'CUX_MG_ID_TRXID'))
       and v.LOOKUP_CODE = 10␀UTE13,
ATTRIBUTE14,
ATTRIBUTE15,
ATTRIBUTE16,
ATTRIBUTE17,
