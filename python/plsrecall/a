       Cux_Gl_Utils_Pkg.Get_Concat_Value(p_Structure_Number => Gcc.Chart_Of_Accounts_Id,
                                         p_Combination_Id   => Gcc.Code_Combination_Id) "¿ÆÄ¿",
       Cux_Gl_Utils_Pkg.Get_Concat_Description(p_Structure_Number => Gcc.Chart_Of_Accounts_Id,
                                               p_Combination_Id   => Gcc.Code_Combination_Id) "¿ÆÄ¿ÃèÊö",
       tr.return_status, tr.return_msg
  FROM Cux_Ar_Customer_Trxs_b_v v,
       Gl_Code_Combinations     Gcc,
       Cux_Ar_Trx_Reverse       Tr
 WHERE v.Customer_Trx_Id = Tr.New_Trx_Id
   AND Tr.Request_Id = Fnd_Global.Conc_Request_Id
   AND v.Code_Combination_Id = Gcc.Code_Combination_Id
   order by "Àà±ð","OU", "ÊÂÎñ´¦Àí±àºÅ", "ÐÐÀà±ð","ÐÐÀàÐÍ"␀as.Ship_To_Flag,
       Cas.Creation_Date Cas_Creation_Date,
       cas.last_update_date cas_last_update_date,
       Ps.Location_Id,
       Ps.Party_Site_Number,
       Ps.Attribute_Category Ps_Attr_Category,
       Ps.Attribute1 Ps_Attr1,
       Ps.Identifying_Address_Flag,
       Ps.Status,
       Ps.Party_Site_Name,
       Lc.Attribute_Category Lc_Attr_Category,
       Lc.Attribute1 Lc_Attr1,
       Lc.Country Lc_Country,
       Terr.Territory_Short_Name Country_Name,
       Lc.Address1 Lc_Address1,
       Lc.Address2 Lc_Address2,
       Lc.City,
       Lc.State,
       Lc.Postal_Code,
       Lc.County,
       Lc.Province
  FROM Hz_Parties               Pty,
       Hz_Organization_Profiles Org,
       Hz_Cust_Accounts         Acc,
       Hz_Cust_Acct_Sites_All   Cas,
       Hz_Party_Sites           Ps,
       Hz_Locations             Lc,
       Fnd_Territories_Vl       Terr,
       Hr_Operating_Units       Ou
 WHERE Org.Party_Id = Pty.Party_Id
   AND Acc.Party_Id = Pty.Party_Id
   AND Acc.Cust_Account_Id = Cas.Cust_Account_Id
   AND Pty.Party_Id = Ps.Party_Id
   AND Cas.Party_Site_Id = Ps.Party_Site_Id
   AND Ps.Location_Id = Lc.Location_Id
   AND Terr.Territory_Code = Lc.Country
   AND Cas.Org_Id = Ou.Organization_Id
   AND EXISTS (SELECT NULL
          FROM Hz_Party_Usg_Assignments Pua
         WHERE Pua.Party_Id = Pty.Party_Id
           AND Pua.Party_Usage_Code = 'CUSTOMER'
           AND Trunc(SYSDATE) BETWEEN Pua.Effective_Start_Date AND
               Pua.Effective_End_Date
           AND Pua.Status_Flag = 'A')␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀␀
