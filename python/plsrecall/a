       END Row_Id,
       'select * from ' || Obj.Owner || '.' || Obj.Object_Name ||
       ' where rowid = ''' || CASE
         WHEN l.Request <> 0 THEN
          Dbms_Rowid.Rowid_Create(1,
                                  Row_Wait_Obj#,
                                  Row_Wait_File#,
