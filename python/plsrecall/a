commit␀into cux_chw_trx_id
select customer_trx_id
  from ra_customer_trx_all rct
      ,cux_chw_run_id ccri
 where 1=1
   and rct.attribute12=ccri.interface_run_id
   and rct.org_id in(select ou2.organization_id
                       from hr_operating_units ou2
                      where ou2.name like '%'||ccri.ou||'%')
union all
SELECT rct2.CUSTOMER_TRX_ID
  FROM ar_receivable_applications_all APP
      ,ra_customer_trx_all rct
      ,ra_customer_trx_all rct2
      ,ra_cust_trx_types_all rctt
      ,cux_chw_run_id ccri
 WHERE 1=1
   and rct.attribute12=ccri.interface_run_id
   and rct2.org_id=rct.org_id
   and rct2.attribute10=rct.attribute10
   and rct2.attribute12 is null
   and rctt.cust_trx_type_id=rct2.cust_trx_type_id
   --and rctt.TYPE='CM'
   and APP.APPLICATION_TYPE = 'CM'
   and (APP.CUSTOMER_TRX_ID =rct2.customer_trx_id or app.applied_customer_trx_id=rct2.customer_trx_id)
   and rct.org_id in(select ou2.organization_id
                       from hr_operating_units ou2
                      where ou2.name like '%'||ccri.ou||'%')␀        attribute15,
                                      object_version_number,
                                      created_by,
                                      creation_date,
                                      last_updated_by,
                                      last_update_date,
                                      last_update_login)values (cux_unify_import_profile_s.nextval,'

||''''||INTERFACE_KEY||''', '||
''''||INTERFACE_DESC||''', '||
''''||FILE_PATH||''', '||
''''||LAYOUT_ID||''', '||
