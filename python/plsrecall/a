       l.Type,
       Lt.Name,
       Lt.Description,
       Lt.Id1_Tag,
       l.Id1,
       Lt.Id2_Tag,
       l.Id2,
       Decode(l.Lmode,
              0,
              ' 0 - None ',
              1,
              ' 1 - NULL(NULL) ',
              2,
