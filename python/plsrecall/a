SELECT DISTINCT /*Ou_Name, Bs_Name, Bs_Description,*/
                              v.Trx_Attr12 /*, v.Created_By*/
                FROM Cux_Ar_Customer_Trxs_b_v v
               WHERE v.Ou_Name LIKE '%PRC%'
                 AND v.Ou_Name NOT LIKE '%QY%PRC%'
                 AND v.Created_By IN (1370, 1129) --('LIUYUJUAN','LVAIPING')␀
              UNION ALL
              SELECT DISTINCT /*Ou.Name,
                              v.Batch_Id,
                              a.Batch_Name,*/ l.Attribute13
                FROM Ap_Invoices_All      v,
                     Hr_Operating_Units   Ou,
                     Ap_Batches_All       a,
                     Ap_Invoice_Lines_All l
               WHERE v.Created_By IN (1370, 1129) --('LIUYUJUAN','LVAIPING');
                 AND Ou.Organization_Id = v.Org_Id
                 AND Ou.Name LIKE '%PRC%'
                 AND Ou.Name NOT LIKE '%QY%PRC%'
                 AND v.Batch_Id = a.Batch_Id
                 AND v.Invoice_Id = l.Invoice_Id␀d
                 AND v.Invoice_Id = l.Invoice_Id) LOOP

