    IF Pov_Return_Status = Fnd_Api.g_Ret_Sts_Success THEN
      COMMIT;
    ELSE
      ROLLBACK;
    END IF;
    UPDATE cux_fa_df_0602_2 d
       SET d.Status     = Pov_Return_Status,
           d.Run_Result = Substr(Pov_Return_Msg, 1, 4000)
     WHERE ROWID = Rec.Row_Id;
    COMMIT;
  END LOOP;
