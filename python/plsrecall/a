   AND (Request_Date >= Trunc(SYSDATE - 7))
   AND (Requestor = 'GAOQIN')
   and s.RESPONSIBILITY_ID = vl.RESPONSIBILITY_ID
   and vl.RESPONSIBILITY_NAME like 'CUX:ÊÕ¿îÈÏÁìÆ½Ì¨%'
